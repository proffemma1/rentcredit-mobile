{
  "expo": {
    "name": "RentCredit",
    "slug": "rentcredit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./generated-icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./generated-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#6366f1"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "runtimeVersion": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rentcredit.app",
      "buildNumber": "1.0.0",
      "deploymentTarget": "13.4"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./generated-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rentcredit.app",
      "versionCode": 1
    },
    "web": {
      "favicon": "./generated-icon.png",
      "bundler": "webpack",
      "output": "single"
    },
    "scheme": "rentcredit",
    "platforms": ["ios", "android", "web"],
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 33,
            "targetSdkVersion": 33,
            "buildToolsVersion": "33.0.0"
          },
          "ios": {
            "deploymentTarget": "13.4"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "6cd9ba47-81c1-4c35-b116-d1dd4b526608"
      }
    }
  }
}
